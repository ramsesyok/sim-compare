{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Timeline",
    "definitions": {
        "Timeline": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "time_sec": {
                    "type": "integer"
                },
                "positions": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/TimelinePosition"
                    }
                }
            },
            "required": [
                "positions",
                "time_sec"
            ],
            "title": "Timeline"
        },
        "TimelinePosition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "object_id": {
                    "type": "string"
                },
                "team_id": {
                    "type": "string"
                },
                "role": {
                    "type": "string"
                },
                "lat_deg": {
                    "type": "number"
                },
                "lon_deg": {
                    "type": "number"
                },
                "alt_m": {
                    "type": "number"
                }
            },
            "required": [
                "alt_m",
                "lat_deg",
                "lon_deg",
                "object_id",
                "role",
                "team_id"
            ],
            "title": "TimelinePosition"
        }
    }
}